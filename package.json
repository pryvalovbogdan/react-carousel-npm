{
  "name": "react-carousel-npm",
  "version": "0.1.0",
  "description": "Carousel with falling on the sides cards",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "prepublish": "rm -rf ./dist && npm run build",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pryvalovbogdan/react-carousel-npm.git"
  },
  "keywords": [
    "carousel",
    "cards"
  ],
  "author": "Bogdan Pryvalov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pryvalovbogdan/react-carousel-npm/issues"
  },
  "homepage": "https://github.com/pryvalovbogdan/react-carousel-npm#readme",
  "devDependencies": {
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.30",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.16",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.9",
    "styled-system": "^5.1.5",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
